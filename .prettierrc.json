{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-prisma"],
  "overrides": [
    {
      "files": "*.prisma",
      "options": {
        "parser": "prisma",
        "singleQuote": false
      }
    }
  ],
  "singleAttributePerLine": true
}
